    <bean id="webscript.gov.uspto.trademark.cms.repo.update.updatedocumentmetadata.post"
          class="gov.uspto.trademark.cms.repo.webscripts.update.UpdateMetadataWebScript"
          parent="webscript">
    </bean>
    <bean id="webscript.gov.uspto.trademark.cms.repo.redaction.redactDocument.post"
          class="gov.uspto.trademark.cms.repo.webscripts.redaction.RedactDocumentWebScript"
          parent="webscript">
    </bean>
    <bean id="webscript.gov.uspto.trademark.cms.repo.redaction.retrieveOriginalContent.get"
          class="gov.uspto.trademark.cms.repo.webscripts.redaction.RetrieveOriginalContentWebScript"
          parent="webscript">
    </bean>
    <bean id="webscript.gov.uspto.trademark.cms.repo.redaction.redactionRestoreOriginal.delete"
