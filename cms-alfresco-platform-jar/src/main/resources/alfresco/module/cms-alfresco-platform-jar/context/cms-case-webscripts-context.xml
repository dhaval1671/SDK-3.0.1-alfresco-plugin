<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<!-- Case folder/s related webscript definitions -->
<beans>
    <bean id="webscript.gov.uspto.trademark.cms.repo.create.createdocument.put"
          class="gov.uspto.trademark.cms.repo.webscripts.create.CreateWebScript"
          parent="webscript">
    </bean>
    <bean id="webscript.gov.uspto.trademark.cms.repo.retrieve.retrievecontent.get"
          class="gov.uspto.trademark.cms.repo.webscripts.retrieve.RetrieveContentWebScript"
          parent="webscript">
    </bean>
    <bean id="webscript.gov.uspto.trademark.cms.repo.retrieve.retrievedocumentattachment.get"
          class="gov.uspto.trademark.cms.repo.webscripts.retrieve.RetrieveDocumentAttachmentWebScript"
          parent="webscript">
    </bean>
    <bean id="webscript.gov.uspto.trademark.cms.repo.retrieve.retrievemetadata.get"
          class="gov.uspto.trademark.cms.repo.webscripts.retrieve.RetrieveMetadataWebScript"
          parent="webscript">
    </bean>
    <bean id="webscript.gov.uspto.trademark.cms.repo.retrieve.retrieveversionslist.get"
          class="gov.uspto.trademark.cms.repo.webscripts.retrieve.RetrieveVersionsListWebScript"
