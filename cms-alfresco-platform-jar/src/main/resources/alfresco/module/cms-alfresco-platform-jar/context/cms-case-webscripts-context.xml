          parent="webscript">
    </bean>
    <bean id="webscript.gov.uspto.trademark.cms.repo.redaction.retrieveRedactContent.get"
          class="gov.uspto.trademark.cms.repo.webscripts.redaction.RetrieveRedactContentWebScript"
          parent="webscript">
    </bean>
    <bean id="webscript.gov.uspto.trademark.cms.repo.delete.deleteDocument.delete"
          class="gov.uspto.trademark.cms.repo.webscripts.delete.DeleteWebScript"
          parent="webscript">
    </bean>    
   
</beans>