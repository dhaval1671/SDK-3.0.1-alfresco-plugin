    <bean id="webscript.gov.uspto.trademark.cms.repo.madrid.retrievemadridcontent.get"
          class="gov.uspto.trademark.cms.repo.webscripts.madrid.RetrieveMadridContentWebScript"
          parent="webscript">
    </bean>    
</beans>